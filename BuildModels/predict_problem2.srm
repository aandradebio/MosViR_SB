#!/usr/bin/bash
#SBATCH --nodes=1                #Número de Nós
#SBATCH --ntasks-per-node=5   #Número de tarefas por Nó
#SBATCH --ntasks=5              #Numero total de tarefas MPI
#SBATCH --cpus-per-task=10
#SBATCH -p labinfo                  #Fila (partition) a ser utilizada
#SBATCH -J predict #Nome job
###SBATCH --time=00:00:30          #Obrigatório

#Exibe os nós alocados para o Job e acessa o diretorio em que o script foi submetido
echo $SLURM_JOB_NODELIST
cd  $SLURM_SUBMIT_DIR

## 1) Carrega o módulo
module load R/4.3.0_bio

arbo_path="/labinfo/home/aandrade/data/datasets/featureMatrices/arboviruses/"
mos_path="/labinfo/home/aandrade/data/datasets/featureMatrices/mosquito/"
models_path="/labinfo/home/aandrade/data/datasets/firstrun/models/"

Rscript predict.R "${arbo_path}featureMatrix_arboviruses_1000bp_12_filtered_test.csv.gz" "${mos_path}featureMatrix_mosquito_subset_1000bp_12_test.csv.gz" "${models_path}class2_1000bp_12" &
Rscript predict.R "${arbo_path}featureMatrix_arboviruses_1000bp_22_filtered_test.csv.gz" "${mos_path}featureMatrix_mosquito_subset_1000bp_22_test.csv.gz" "${models_path}class2_1000bp_22" &

Rscript predict.R "${arbo_path}featureMatrix_arboviruses_500bp_12_filtered_test.csv.gz" "${mos_path}featureMatrix_mosquito_subset_500bp_12_test.csv.gz" "${models_path}class2_500bp_12" &
Rscript predict.R "${arbo_path}featureMatrix_arboviruses_500bp_13_filtered_test.csv.gz" "${mos_path}featureMatrix_mosquito_subset_500bp_13_test.csv.gz" "${models_path}class2_500bp_13" &

Rscript predict.R "${arbo_path}featureMatrix_arboviruses_3000bp_13_filtered_test.csv.gz" "${mos_path}featureMatrix_mosquito_subset_3000bp_13_test.csv.gz" "${models_path}class2_3000bp_13" &

wait
